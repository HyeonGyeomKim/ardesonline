<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.ardes.mapper.GetTokenMapper">

    <select id="getUserByToken" resultType="java.util.Map">
        SELECT user_name
        FROM ardes_online_db
        WHERE Token = #{token}
    </select>
</mapper>
